{
  "name": "tft",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "downshift": "^3.2.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
